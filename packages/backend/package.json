{
  "name": "backend",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:ci": "npm test -- --ci --coverage",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "ci": "run-s lint format test:ci",
    "lint": "eslint \"**/*.js\"",
    "format": "prettier --check --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "eslint": "9.11.1",
    "globals": "15.9.0",
    "jest": "29.7.0",
    "npm-run-all": "4.1.5"
  },
  "dependencies": {
    "prettier": "3.3.3"
  }
}
